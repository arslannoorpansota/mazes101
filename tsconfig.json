{
  "compilerOptions": {
    "declaration": false,
    "outDir": "dist",
    "module": "ESNext",
    "moduleResolution": "node",
    "target": "ES2019",
    "jsx": "react",
    "jsxFactory": "h",
    "plugins": [
      { "transform": "ts-transformer-keys/transformer" }
    ],
    "lib": [
      "ES2019",
      "dom"
    ]
  },
  "include": [
    "src/**/*.ts"
  ]
}
