{
  "name": "mazes101",
  "version": "0.0.0-development",
  "description": "Utilities to generate, render and hack with any sort of mazes",
  "main": "index.cjs",
  "module": "index.js",
  "types": "index.d.ts",
  "files": [
    "**"
  ],
  "scripts": {
    "post-clean": "find dist -type f ! -name 'mazes101.d.ts' -name '**.d.ts' -delete && find dist -empty -type d -delete",
    "build": "rm -rf dist && rollup --config && npm run post-clean && mv dist/mazes101.d.ts dist/index.d.ts",
    "watch": "ttsc --watch",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:nmanumr/mazes101.git"
  },
  "keywords": [
    "maze",
    "maze-puzzle",
    "circular-maze",
    "rectangular-maze",
    "game"
  ],
  "author": "nmanumr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nmanumr/mazes101/issues"
  },
  "homepage": "https://nmanumr.github.io/mazes101/",
  "devDependencies": {
    "preact": "^10.5.14",
    "rollup": "^2.47.0",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "semantic-release": "^17.4.2",
    "ts-transformer-keys": "^0.4.3",
    "tslib": "^2.2.0",
    "tsutils": "^3.21.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.4"
  },
  "dependencies": {}
}
